    {
      "id": "haruki-z-kai-202511-0032",
      "ja": "とても",
      "en": "very"
    },
    {
      "id": "haruki-z-kai-202511-0033",
      "ja": "楽しい",
      "en": "fun"
    },
    {
      "id": "haruki-z-kai-202511-0034",
      "ja": "旅行",
      "en": "trip"
    },
    {
      "id": "haruki-z-kai-202511-0035",
      "ja": "わたしたちは京都を訪れました。",
      "en": "We visited Kyoto."
    },
    {
      "id": "haruki-z-kai-202511-0036",
      "ja": "わたしたちは金閣寺を見学しました。",
      "en": "We toured Kinkaku-ji."
    },
    {
      "id": "haruki-z-kai-202511-0037",
      "ja": "わたしたちは紅葉を見ました。",
      "en": "We saw autumn leaves."
    },
    {
      "id": "haruki-z-kai-202511-0038",
      "ja": "わたしは友だちに会いました。",
      "en": "I met my friend."
    },
    {
      "id": "haruki-z-kai-202511-0039",
      "ja": "僕たちは夕食にすしを食べました。",
      "en": "We ate sushi for dinner."
    },
    {
      "id": "haruki-z-kai-202511-0040",
      "ja": "わたしたちはお茶を飲みました。",
      "en": "We drank tea."
    },
    {
      "id": "haruki-z-kai-202511-0041",
      "ja": "わたしたちはおみやげを買いました。",
      "en": "We bought souvenirs."
    },
    {
      "id": "haruki-z-kai-202511-0042",
      "ja": "僕たちは写真を撮りました。",
      "en": "We took photos."
    },
    {
      "id": "haruki-z-kai-202511-0043",
      "ja": "とても楽しかったです。",
      "en": "It was very fun."
    },
    {
      "id": "haruki-z-kai-202511-0044",
      "ja": "わたしたちは彼と話しました。",
      "en": "We talked with him."
    },
    {
      "id": "haruki-z-kai-202511-0045",
      "ja": "わたしたちは彼と昼食を食べました。",
      "en": "We had lunch with him."
    },
    {
      "id": "haruki-z-kai-202511-0046",
      "ja": "ぼくたちは彼女と夕食を食べました。",
      "en": "We had dinner with her."
    },
    {
      "id": "haruki-z-kai-202511-0047",
      "ja": "ぼくたちは彼女の国について話しました。",
      "en": "We talked about her country."
    },
    {
      "id": "haruki-z-kai-202511-0048",
      "ja": "とても興味深かったです。",
      "en": "It was very interesting."
